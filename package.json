{
  "name": "Hangload",
  "version": "1.0.0",
  "private": true,
  "description": "Hangload -- The new way to load your bike",
  "homepage": "http://hangload.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/hangload/static"
  },
  "author": "bodokaiser <i@bodokaiser.io>",
  "contributors": [
    {
      "name": "mrzool",
      "email": "info@mrzool.com"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "browser-sync": "^2.9.6",
    "node-minify": "^1.2.1",
    "parallelshell": "^2.0.0",
    "postcss": "^5.0.8",
    "postcss-browser-reporter": "^0.4.0",
    "postcss-conditionals": "^2.0.0",
    "postcss-css-variables": "^0.5.0",
    "postcss-custom-media": "^5.0.0",
    "postcss-import": "^7.0.0",
    "watch": "^0.16.0"
  },
  "scripts": {
    "prestart": "npm run clean",
    "start": "parallelshell -v 'npm run build:css' 'npm run build:html' 'npm run server'",
    "build:html": "bundle exec jekyll build --watch --incremental",
    "build:css": "watch 'node css.js' src/",
    "clean": "rm -rf _site",
    "server": "browser-sync start --files '_site/**/*.html' --server _site --no-notify"
  },
  "dependencies": {
    "bootstrap": "^3.1.1",
    "colors.css": "^3.0.0",
    "gravitons": "^1.0.0"
  }
}
